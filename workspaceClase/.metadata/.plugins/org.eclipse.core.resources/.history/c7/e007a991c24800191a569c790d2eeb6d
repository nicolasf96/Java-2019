package tarea1;

public class Principal {

	public static void main(String[] args) {
		
		System.out.println("Esto muestra los numeros del 1 al 10");
		
		for(int n = 1;n < 11; n++){
			
			System.out.print(n);
			
		}  

		
System.out.println("\nEsto muestra los numeros impares del 1 al 10");
		
		for(int n = 1;n < 11; n+=2){
			
			System.out.print(n);
			
		}

	}

}
